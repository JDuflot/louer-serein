<form method="post" action="{{ path('app_rental_delete', {'id': rental.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer votre location ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rental.id) }}">
    <button class="btn btn-danger col-2 mt-3 mb-3">Supprimer</button>
</form>
